<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="2.5dip" />
            <gradient android:startColor="#00000000" android:endColor="#00000000" android:angle="270.0" android:centerY="0.75" android:centerColor="#00000000" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.5dip" />
                <gradient android:startColor="?android:colorEdgeEffect" android:endColor="?android:colorEdgeEffect" android:angle="270.0" android:centerY="0.8" android:centerColor="?android:colorEdgeEffect" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.5dip" />
                <gradient android:startColor="?android:colorControlActivated" android:endColor="?android:colorControlActivated" android:angle="270.0" android:centerY="0.8" android:centerColor="?android:colorControlActivated" />
            </shape>
        </clip>
    </item>
</layer-list>

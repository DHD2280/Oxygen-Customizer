<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:end="24dp"
        android:start="24dp">
        <shape
            android:shape="rectangle"
            android:tint="?android:attr/colorBackgroundFloating">
            <solid android:color="#66FFFFFF" />
            <corners
                android:bottomLeftRadius="@*android:dimen/harmful_app_name_padding_top"
                android:bottomRightRadius="@*android:dimen/harmful_app_name_padding_top"
                android:topLeftRadius="@*android:dimen/harmful_app_name_padding_left"
                android:topRightRadius="@*android:dimen/harmful_app_name_padding_left" />
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:end="12dp"
        android:start="12dp">
        <shape
            android:shape="rectangle"
            android:tint="?android:attr/colorBackgroundFloating">
            <solid android:color="#B3FFFFFF" />
            <corners
                android:bottomLeftRadius="@*android:dimen/harmful_app_name_padding_top"
                android:bottomRightRadius="@*android:dimen/harmful_app_name_padding_top"
                android:topLeftRadius="@*android:dimen/harmful_app_name_padding_left"
                android:topRightRadius="@*android:dimen/harmful_app_name_padding_left" />
        </shape>
    </item>
    <item android:bottom="12dp">
        <shape android:shape="rectangle">
            <solid android:color="?android:attr/colorBackgroundFloating" />
            <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
        </shape>
    </item>
</layer-list>